{
    "comments": {
        "lineComment": "#",
        "blockComment": [ "#[", "]#" ]
    },
    "brackets": [
        ["[", "]"],
        ["(", ")"]
    ],
    "autoClosingPairs": [
        ["#[", "]#"],
        {"open": "[", "close": "]", "notIn": ["comment"]},
        ["(", ")"],
        { "open": "\"", "close": "\"", "notIn": ["string", "comment"] }
    ],
    "surroundingPairs": [
        ["[", "]"],
        ["(", ")"],
        ["'", "'"],
        ["\"", "\""]
    ],
    "folding": {
        "offSide": true
    }
}
